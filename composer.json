{
    "name": "bolt/file-browser",
    "description": "File & directory browser",
    "type": "bolt-extension",
    "keywords": [
        "browse",
        "directories",
        "download",
        "files"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gawain Lynch",
            "email": "gawain.lynch@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Bolt\\FileDirectoryBrowser\\": "src"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Bolt\\FileDirectoryBrowser\\FileDirectoryBrowserExtension",
        "bolt-icon": "icon.png",
        "bolt-screenshots": [
            "screenshot-main.png",
            "screenshot-subdirectory.png"
        ],
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
